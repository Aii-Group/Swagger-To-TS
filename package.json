{
  "name": "swagger-to-ts-axios",
  "version": "1.0.0",
  "description": "Generate TypeScript interfaces from Swagger/OpenAPI and create type-safe axios wrappers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "swagger-to-ts": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist",
    "commit": "git-cz",
    "prepare": "husky install",
    "lint:commit": "commitlint --from HEAD~1 --to HEAD --verbose"
  },
  "keywords": [
    "swagger",
    "openapi",
    "typescript",
    "axios",
    "codegen",
    "api",
    "type-safe"
  ],
  "author": "AII Group",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.0.0",
    "@commitlint/config-conventional": "^18.0.0",
    "@types/fs-extra": "^11.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.0.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "jest": "^29.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
